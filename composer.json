{
    "name": "technologi/assesment",
    "type": "project",
    "description": "Technolgi Assesment",
    "keywords": ["assesment"],
    "license": "MIT",
    "require": {
      "php": "^7.4"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
      "files": [
        "app/helpers.php"
      ],
      "psr-4": {
        "App\\": "app/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": [
        "phpunit"
      ]
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true,
      "allow-plugins": {
        "composer/package-versions-deprecated": false
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
  